					<tickRate>60</tickRate>
					<needsToAdjust>
						<Food>0.25</Food>
						<Rest>0.25</Rest>
						<Joy>0.25</Joy>
						<Outdoors>0.5</Outdoors>
					</needsToAdjust>
				</needAdjustProperties>
				<healingProperties>
					<healPoints>5</healPoints>
					<!-- <hediffsToHeal>3</hediffsToHeal> -->
