				<effectWhenDowned>
					<hediff>FE_FusionReactor</hediff>
					<apply>10</apply>
					<ticksBetweenActivations>666</ticksBetweenActivations>
					<showDownActivationTimer>true</showDownActivationTimer>
				</effectWhenDowned>
				<repairProperties>
