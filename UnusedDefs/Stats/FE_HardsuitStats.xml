<Defs>

	<StatCategoryDef>
		<defName>FE_ResourceLabel</defName>
		<label>Resource</label>
		<displayOrder>0</displayOrder>
		<displayAllByDefault>true</displayAllByDefault>
	</StatCategoryDef>

	<StatDef>
		<defName>FE_FusionReactorPerSecond</defName>
		<workerClass>ART.StatWorker_ResourceOffset</workerClass> 
		<label>energy per second</label>
		<description>Amount of energy generated per second.</description>
		<category>FE_ResourceLabel</category>
		<displayPriorityInCategory>10</displayPriorityInCategory>
		<toStringStyle>FloatTwo</toStringStyle>
		<defaultBaseValue>0</defaultBaseValue>
		<modExtensions>
			<li Class="ART.StatWorkerExtender">
				<hediffResource>FE_FusionReactor</hediffResource>
			</li>
		</modExtensions>
	</StatDef>
	
	<StatDef>
		<defName>FE_FusionReactorCapacity</defName>
		<workerClass>ART.StatWorker_MaxResourceCapacityOffset</workerClass>
		<label>energy capacity</label>
		<description>Maximum amount of energy.</description>
		<category>FE_ResourceLabel</category>
		<displayPriorityInCategory>9</displayPriorityInCategory>
		<toStringStyle>FloatTwo</toStringStyle>
		<defaultBaseValue>0</defaultBaseValue>
		<modExtensions>
			<li Class="ART.StatWorkerExtender">
				<hediffResource>FE_FusionReactor</hediffResource>
			</li>
		</modExtensions>
	</StatDef>
	
	
